{
	"name": "a2i2a",
	"displayName": "a2i2a",
	"description": "快速把apidoc接口文档的数据生成typescript的interface 和 接口文件 🚀 🚀 🚀",
	"publisher": "fyang",
	"version": "0.0.1",
	"main": "./dist/extension.js",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Notebooks",
		"Other"
	],
	"activationEvents": [
		"onView:api"
	],
	"contributes": {
		"commands": [
			{
				"command": "api.addApi",
				"title": "添加接口地址",
				"icon": "$(add)"
			},
			{
				"command": "api.refreshApi",
				"title": "刷新接口",
				"icon": "$(refresh)"
			},
			{
				"command": "api.apiBuild",
				"title": "生成接口文件",
				"icon": "$(tools)"
			},
			{
				"command": "api.removeNode",
				"title": "删除节点",
				"icon": "$(trash)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "fast_api",
					"title": "a2i2a",
					"icon": "icon.svg"
				}
			]
		},
		"views": {
			"fast_api": [
				{
					"id": "api",
					"name": "接口列表"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "api",
				"contents": "欢迎使用a2i2a \n[添加接口](command:api.addApi)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "api.addApi",
					"when": "view == api",
					"group": "navigation@0"
				}
			],
			"view/item/context": [
				{
					"command": "api.refreshApi",
					"when": "view == api && viewItem == dir",
					"group": "inline@0"
				},
				{
					"command": "api.removeNode",
					"when": "view == api && viewItem == dir",
					"group": "inline@1"
				},
				{
					"command": "api.apiBuild",
					"when": "viewItem == module",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
		"package:vise": "vsce package",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"axios": "^0.21.0",
		"typeof-jsonc": "^1.1.10"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-loader": "^8.0.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
